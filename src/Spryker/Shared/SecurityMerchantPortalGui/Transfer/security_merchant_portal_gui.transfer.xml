<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="MerchantUserCriteria">
        <property name="withUser" type="bool"/>
        <property name="username" type="string"/>
        <property name="status" type="string"/>
        <property name="merchantStatus" type="string"/>
    </transfer>

    <transfer name="MerchantUser">
        <property name="user" type="User"/>
    </transfer>

    <transfer name="User">
        <property name="username" type="string"/>
        <property name="password" type="string"/>
    </transfer>

    <transfer name="Message">
        <property name="value" type="string"/>
    </transfer>

    <transfer name="UserPasswordResetRequest">
        <property name="resetPasswordPath" type="string"/>
        <property name="email" type="string"/>
    </transfer>

    <transfer name="Group">
        <property name="name" type="string"/>
    </transfer>

    <transfer name="Rule">
        <property name="bundle" type="string"/>
        <property name="controller" type="string"/>
        <property name="action" type="string"/>
        <property name="type" type="string"/>
    </transfer>

    <transfer name="SecurityCheckAuthContext">
        <property name="type" type="string"/>
        <property name="ip" type="string"/>
        <property name="account" type="string"/>
    </transfer>

    <transfer name="SecurityCheckAuthResponse">
        <property name="isBlocked" type="bool"/>
        <property name="numberOfAttempts" type="int"/>
        <property name="blockedFor" type="int"/>
        <property name="securityCheckAuthContext" type="SecurityCheckAuthContext"/>
    </transfer>

    <transfer name="AuditLoggerConfigCriteria" strict="true">
        <property name="channelName" type="string"/>
    </transfer>

    <transfer name="MultiFactorAuthValidationResponse" strict="true">
        <property name="isRequired" type="bool"/>
        <property name="status" type="int"/>
    </transfer>

    <transfer name="MultiFactorAuthValidationRequest" strict="true">
        <property name="user" type="User"/>
    </transfer>

    <transfer name="ZedUiFormResponse" strict="true">
    </transfer>

    <transfer name="ZedUiFormRequestAction" strict="true">
        <property name="url" type="string"/>
        <property name="isLogin" type="bool"/>
        <property name="formSelector" type="string"/>
    </transfer>

</transfers>
