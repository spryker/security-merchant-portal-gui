{
  "name": "spryker/security-merchant-portal-gui",
  "type": "library",
  "description": "SecurityMerchantPortalGui module",
  "license": "proprietary",
  "require": {
    "php": ">=7.3",
    "spryker/kernel": "^3.33.0",
    "spryker/merchant-user": "^0.6.1",
    "spryker/messenger": "^3.0.0",
    "spryker/security": "^1.3.0",
    "spryker/security-extension": "^1.0.0",
    "spryker/symfony": "^3.5.0",
    "spryker/zed-ui": "^0.3.9"
  },
  "require-dev": {
    "spryker/application": "*",
    "spryker/code-sniffer": "*",
    "spryker/container": "*",
    "spryker/customer": "*",
    "spryker/event-dispatcher": "*",
    "spryker/form": "*",
    "spryker/merchant": "*",
    "spryker/propel": "*",
    "spryker/router": "*",
    "spryker/session": "*",
    "spryker/testify": "*",
    "spryker/user": "*",
    "spryker/validator": "*"
  },
  "suggest": {
    "spryker/container": "Add this module when you want to use the Container."
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SprykerTest\\": "tests/SprykerTest/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/ tests/",
    "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/ tests/"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
